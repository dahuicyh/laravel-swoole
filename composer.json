{
    "name": "swoole/lumen-swoole",
    "description": "High performance HTTP server based on Swoole. Speed up your Laravel and Lumen applications.",
    "keywords": ["swoole", "laravel", "lumen"],
    "license": "MIT",
    "authors": [
        {
            "name": "david",
            "email": "dahuicyh@qq.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/console": "~5.1",
        "illuminate/contracts": "~5.1",
        "illuminate/http": "~5.1",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "laravel/lumen-framework": "~5.1",
        "phpunit/phpunit": "^6.1",
        "phpunit/php-code-coverage": "^5.2",
        "satooshi/php-coveralls": "^1.0",
        "mockery/mockery": "~1.0",
        "codedungeon/phpunit-result-printer": "^0.14.0",
        "php-mock/php-mock": "^2.0"
    },
    "autoload": {
        "files": [
            "src/Server/helpers.php"
        ],
        "psr-4": {
            "Swoole\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swoole\\Http\\Tests\\": "tests",
            "Swoole\\Http\\Tests\\Fixtures\\Laravel\\App\\": "tests/fixtures/laravel/app"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Swoole\\Http\\LaravelServiceProvider"
            ],
            "aliases": {
                "Server": "Swoole\\Http\\Server\\Facades\\Server",
                "Table": "Swoole\\Http\\Server\\Facades\\Table",
                "Room": "Swoole\\Http\\Websocket\\Facades\\Room",
                "Websocket": "Swoole\\Http\\Websocket\\Facades\\Websocket"
            }
        }
    }
}
